<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="pp.file" class="java.util.Properties" >
		<constructor-arg>
			<props>
				<prop key="baseDir">d:/isfasiel/upload</prop>
				<prop key="mineType">exe,com,jsp,js,html,htm,asp,aspx</prop>
			</props>
		</constructor-arg>
	</bean>
	
	<bean id="pp.security" class="java.util.Properties" >
		<constructor-arg>
			<props>
				<prop key="keyFileName">isfasielKey.key</prop>
			</props>
		</constructor-arg>
	</bean>
	
	<bean id="pp.page" class="java.util.Properties" >
		<constructor-arg>
			<props>
				<prop key="pageSize">30</prop>
				<prop key="labelSize">10</prop>
				<prop key="icon">
<![CDATA[<a href="#"  class="<CSS>" onclick="<ONCLICK>"><DESIGN></a>]]> 
				</prop>
			</props>
		</constructor-arg>
	</bean>

	<bean id="group.error" class="java.util.Properties" >
		<constructor-arg>
			<props>
				<prop key="LOGIN_ERROR"><![CDATA[LOGIN_ERROR]]></prop>
				<prop key="DUPLICATED_NAME"><![CDATA[DUPLICATED_NAME]]></prop>
				<prop key="NO_GROUP_NAME"><![CDATA[NO_GROUP_NAME]]></prop>
				<prop key="NO_GROUP_ID"><![CDATA[NO_GROUP_ID]]></prop>
				<prop key="NO_GROUP"><![CDATA[NO_GROUP]]></prop>
<!--				<prop key=""><![CDATA[]]></prop>-->
			</props>
		</constructor-arg>
	</bean>
	
	<bean id="groupPath.error" class="java.util.Properties" >
		<constructor-arg>
			<props>
				<prop key="LOGIN_ERROR"><![CDATA[LOGIN_ERROR]]></prop>
				<prop key="DUPLICATED_NAME"><![CDATA[DUPLICATED_NAME]]></prop>
				<prop key="NO_PATH_NAME"><![CDATA[NO_PATH_NAME]]></prop>
				<prop key="NO_GROUP_ID"><![CDATA[NO_GROUP_ID]]></prop>
				<prop key="NO_PATH_ID"><![CDATA[NO_PATH_ID]]></prop>
<!--				<prop key=""><![CDATA[]]></prop>-->
			</props>
		</constructor-arg>
	</bean>
	<bean id="movie.error" class="java.util.Properties" >
		<constructor-arg>
			<props>
				<prop key="LOGIN_ERROR"><![CDATA[LOGIN_ERROR]]></prop>
				<prop key="DUPLICATED_NAME"><![CDATA[DUPLICATED_NAME]]></prop>
				<prop key="NO_PATH_NAME"><![CDATA[NO_PATH_NAME]]></prop>
				<prop key="NO_GROUP_ID"><![CDATA[NO_GROUP_ID]]></prop>
				<prop key="NO_PATH_ID"><![CDATA[NO_PATH_ID]]></prop>
<!--				<prop key=""><![CDATA[]]></prop>-->
			</props>
		</constructor-arg>
	</bean>

</beans>
